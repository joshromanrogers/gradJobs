<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>GradJobs</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="/style.css">
</head>

<body>
    <h1>GradJobsüéì</h1>
    <a href="/postJob" class="button">Post A Job</a>

    <!-- search bar -->
    <form action="/" method="GET">
        <div class="search">
            <input class="searchTerm" type="text" name="search" placeholder="">
            <button class="searchButton" type="submit" value="search">
                <%# <i class="fa fa-search"></i> %>
                üîç
            </button>
        </div>
    </form>

    <!-- categories -->
    <ul class="categories-container">
        <li class="active category"><a href="/">All</a></li>
        <li class="category"><a href="/tech">Tech</a></li>
        <li class="category"><a href="/design">Design</a></li>
        <li class="category"><a href="/finance">Finance</a></li>
        <li class="category"><a href="/marketing">Marketing</a></li>
        <li class="category"><a href="/engineering">Engineering</a></li>
    </ul>

    <!-- job table -->
    <table>

    <colgroup>
       <col span="1" style="width: 50%;">
       <col span="1" style="width: 30%;">
       <col span="1" style="width: 10%;">
       <col span="1" style="width: 10%;">

    </colgroup>

        <% data.forEach(d => { %>
        <tr class="main-row">
            <td><b><%= d.title %></b></td>
            <td><%= d.categories %></td>

            <td><%= moment(d.createdAt).fromNow(true) %></td>
            <td><a class="apply" href=" <%= d.url %> ">Apply</a></td>
        </tr>
        <tr class="description-row"">
            <td>
                <div><%= d.description %></div>

            </td>
        </tr>
        <% }); %>


    </table>
    <script type=" module" src="../index.js">
            </script>
            <script type="text/javascript" src="/script.js"></script>
            <script type="module" src="../util/regex.js"></script>
            <!-- <script type="text/javascript" src="../index.js"></script> -->


</body>

</html>

